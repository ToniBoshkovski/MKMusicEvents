@model List<MKMusicEvents.Models.Event>

@{
    ViewBag.Title = "Favorites";
}

@foreach (var item in Model)
{
    <h1>@item.Name</h1>
    <p>@item.Date</p>
    <p>@item.Description</p>
}


<script>
    $(document).ready(function () {
        $('.bottom').removeClass('bottom').addClass('fixed-bottom');
    });
</script>
